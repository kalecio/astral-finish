{
  "name": "Astral Finish",
  "description": "Extension to get your lobby in Blazblue Central Fiction",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/burst_16.png",
      "32": "/images/burst_32.png",
      "48": "/images/burst_48.png",
      "128": "/images/burst_128.png"
    }
  },
  "icons": {
    "16": "/images/burst_16.png",
    "32": "/images/burst_32.png",
    "48": "/images/burst_48.png",
    "128": "/images/burst_128.png"
  }
}